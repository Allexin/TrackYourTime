{
  "manifest_version": 2,

  "name": "TrackYourTime Chrome Tracker",
  "description": "This extension collect information about visited sites and send it to TrackYourTime",
  "version": "2.3",
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  },
  "icons": {
    "128": "main.png"
  },
  "browser_action": {
    "default_icon": "main.png"
  },
  "permissions": [
    "tabs",
    "alarms",
    "http://127.0.0.1/"
  ]
}
